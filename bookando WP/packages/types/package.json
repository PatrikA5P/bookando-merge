{
  "name": "@bookando/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for Bookando platform (WordPress, SaaS, Mobile)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./models": {
      "import": "./dist/models.js",
      "types": "./dist/models.d.ts"
    },
    "./enums": {
      "import": "./dist/enums.js",
      "types": "./dist/enums.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "bookando",
    "types",
    "typescript",
    "shared"
  ],
  "author": "Bookando Team",
  "license": "PROPRIETARY",
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
