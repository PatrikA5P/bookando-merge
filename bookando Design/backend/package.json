  "author": "Bookando",
  "license": "MIT",
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@prisma/client": "^5.8.0",
    "@sendgrid/mail": "^8.1.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "googleapis": "^170.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.12",
    "socket.io": "^4.6.0",
    "stripe": "^14.10.0",
    "tsdav": "^2.1.6"
  },
  "devDependencies": {
